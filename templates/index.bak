<!DOCTYPE html>
<html lang="en">

  <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" rel="stylesheet">
  <link rel=stylesheet type=text/css href="{{ url_for('static', filename='main.css') }}">


  <body>
    <div class="container">
      
      <div class="header">
        <h3 class="text-muted">Small Cell JFW Attenuator Control</h3>
      </div>
      <hr/>

      <div id="slider">
        {% for values in attens %}
            {% if loop.index <=8 %} 
	    <label for="rangeinput">Attenuator:  {{loop.index}} </label>
            <input id="rangeinput" type="range" min="0" value={{values}} max="127" step="1" onchange="rangeinput.value=value" />
            <output id = "rangevalue">{{values}}</output>
	    {% endif %}
	{% endfor %}
      
      </div>

      <div id="slider">
        {% for values in attens %}
            {% if loop.index >=9 %}
            <label for="rangeinput">Attenuator:  {{loop.index}} </label>
            <input id="rangeinput" type="range" min="0" value={{values}} max="127" step="1" onchange="rangeinput.value=value" />
            <output id = "rangevalue">{{values}}</output>
            {% endif %}
        {% endfor %}
      </div>

      <div id="testing">
	    <form>
	    <input type="range" name="amountRange" min="0" max="10" value="5" oninput="this.form.amountInput.value=this.value" />
	    <input type="number" name="amountInput" min="0" max="10" value="5" oninput="this.form.amountRange.value=this.value" />
	    </form>
      </div>

      <hr/>    
      
      </div>
    
    </div>
  </body>
</html>
 
